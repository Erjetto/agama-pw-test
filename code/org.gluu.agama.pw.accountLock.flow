Flow org.gluu.agama.pw.accountLock
     Basepath ""
Log "@info Start execution of account lock flow"
uiFeedback = {}
validationService = Call org.gluu.agama.pw.PasswordService#getInstance conf
// Iterate x times max
Repeat 3 times max
